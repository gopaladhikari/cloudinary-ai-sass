generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Video {
  id             String   @id @default(uuid())
  title          String
  description    String?
  publicId       String
  originalSize   Int
  compressedSize Int
  duration       Int
  createdAt      DateTime @default(now())
  updatedAt      DateTime @updatedAt
}
